{
    "name": "moove",
    "version": "0.1.0",
    "description": "moves a file somewhere else.",
    "author": "ɛʟʟɨօȶ b",
    "license": "The Unlicense",
    "keywords": [
        "move",
        "files",
        "stuff",
        "renaming",
        "move",
        "cli",
        "hentai"
    ],
    "os": [
        "linux",
        "darwin"
    ],
    "scripts": {
        "test": "node ./test.js",
        "build": "pkg . --targets node18-linux-x64,node18-macos-arm64,node18-macos-x64 --output ./bin/moove"
    },
    "module": "./index.js",
    "bin": {
        "move": "./index.js"
    },
    "main": "index.js",
    "type": "module",
    "dependencies": {
        "async": "^3.2.5",
        "chalk": "^5.3.0",
        "prompt-sync": "^4.2.0",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^2.7.1",
        "release-it": "^16.2.1"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "man": "./moove.1",
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "extends": [
            "eslint:recommended",
            "prettier"
        ],
        "env": {
            "es2021": true,
            "node": true,
            "es6": true
        },
        "rules": {
            "no-unused-vars": "warn",
            "no-console": "off"
        }
    },
    "publishConfig": {
        "executableFiles": [
            "./index.js"
        ],
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release v${version}"
        },
        "github": {
            "release": true
        }
    }
}
